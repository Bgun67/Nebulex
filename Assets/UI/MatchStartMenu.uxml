                <ui:VisualElement style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); flex-basis: 80%; background-image: url(&apos;project://database/Assets/Textures/Map%20Images/LHX-Ultima-Base.JPG?fileID=2800000&amp;guid=3bac2246d927eea4a943707437fe01cd&amp;type=3#LHX-Ultima-Base&apos;); -unity-background-scale-mode: scale-and-crop;" />
                <ui:VisualElement name="HLayout" style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); flex-direction: row; flex-basis: auto; height: 0; justify-content: flex-end; max-height: 30px;">
