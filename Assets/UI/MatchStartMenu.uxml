<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <Style src="project://database/Assets/UI/styles/GlobalStyle.uss?fileID=7433441132597879392&amp;guid=fe218c745313f3642961a1878338522e&amp;type=3#GlobalStyle" />
    <ui:VisualElement name="BackgroundImage" style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); background-image: url(&apos;project://database/Assets/Textures/Skybox/starry-background-high-res.jpg?fileID=2800000&amp;guid=b27c48243f42d6f45bb501ac4de9024e&amp;type=3#starry-background-high-res&apos;);">
        <ui:VisualElement name="Background" style="flex-grow: 1; background-color: rgba(103, 103, 103, 0.62); flex-direction: row; background-image: none; -unity-text-outline-color: rgba(108, 108, 108, 0);">
